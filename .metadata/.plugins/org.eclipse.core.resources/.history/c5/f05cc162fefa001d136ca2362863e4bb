package application.controlleurs;

import application.EchangeurDeVue;
import application.vues.EnsembleDesVues;

import javafx.fxml.FXML;
import javafx.scene.text.Text;

/**
 * Contrôle via FXML les interactions avec la vue : les pages FXML
 * 
 * @author Tom Douaud
 * @author Loïc Faugières
 * @author Simon Guiraud
 */
public class ControlleurParametres {

	@FXML
	private Text menuprincipal_infospartie;
	
	@FXML
	private void clickParametres() {		
		menuprincipal_infospartie.setText("test");
		
		// échanger la vue courante avec celle des paramètres
		EchangeurDeVue.echangerAvec(EnsembleDesVues.VUE_PARAMETRES);
	}
	
	@FXML
	private void generer() {
		
	}

}